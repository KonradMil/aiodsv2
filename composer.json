{
    "name": "aiodsv2/core",
    "description": "Core backend components for AIODS framework",
    "type": "library",
    "keywords": [
        "aiods",
        "crud",
        "laravel",
        "framework",
        "permissions",
        "audit",
        "settings"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "AIODS Team"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "spatie/laravel-permission": "^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Aiods\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aiods\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aiods\\Core\\AiodsCoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

